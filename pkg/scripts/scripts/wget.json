{
    "name": "wget",
    "version": "1.24.5",
    "license": "MIT",
    "open_source": true,
    "description": "Network utility to retrieve files from the Web",
    "source": "https://www.gnu.org/software/wget/wget.html",
    "dependencies": [""],
    "install": {
        "cloning": [
            "curl -O https://ftp.gnu.org/gnu/wget/wget2-2.1.0.tar.gz"
        ],
        "building": [
            "tar xzf wget2-2.1.0.tar.gz",
            "cd wget2-2.1.0 && ./configure --prefix=/usr --sysconfdir=/etc --disable-rpath --enable-nls --with-ssl=gnutls && make"
        ]
    },
    "uninstall": [
        ""
    ]
}
